<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pascal.Mannitok">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.pascal.Mannitok.PremiereActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.pascal.Mannitok.MainActivity" />
        <activity android:name="com.example.pascal.Mannitok.DetailsActivity" />
        <activity android:name="com.example.pascal.Mannitok.LoginActivity" />
        <activity android:name="com.example.pascal.Mannitok.CoursEventActivity" />
        <activity android:name="com.example.pascal.Mannitok.CreateEventActivity" />
        <activity android:name="com.example.pascal.Mannitok.PreferencesActivity" />
        <activity android:name="com.example.pascal.Mannitok.EventDetailsNotAuteurActivity" />
        <activity android:name="com.example.pascal.Mannitok.EventDetailsAuteurActivity" />
        <activity android:name="com.example.pascal.Mannitok.ModifyEventActivity"></activity>
        <activity android:name="com.example.pascal.Mannitok.UserActivity"></activity>
        <activity android:name="com.example.pascal.Mannitok.MarchandActivity"></activity>
        <activity android:name="com.example.pascal.Mannitok.EntrepriseActivity"></activity>
    </application>

</manifest>
